    "version": "0.2.0",
    "configurations": [
        {
            "name": "generate-ts-lib",
            "console": "debugConsole",
            "cwd": "${workspaceFolder}",
            "request": "launch",
            "type": "dart",
            "program": "generate/bin/generate_ts.dart",
            "args": [
                "--path",
                "${workspaceFolder}\\zac\\packages\\zac\\lib\\src",
                "--headFile",
                "${workspaceFolder}\\zac_js\\src\\header.ts",
                "--classesFile",
                "${workspaceFolder}\\zac_js\\src\\classes.ts",
                "--jsRoot",
                "${workspaceFolder}\\zac_js"
            ],
        },
        {
            "name": "generate-builder",
            "cwd": "${workspaceFolder}",
            "request": "launch",
            "type": "dart",
            "program": "generate/bin/generate_registry.dart",
            "args": [
                "--path",
                "${workspaceFolder}\\zac\\packages\\zac\\lib\\src",
                "--outFile",
                "${workspaceFolder}\\zac\\packages\\zac\\lib\\src\\zac\\generated_registry.dart"
            ],
        }
    ]
}